#include<stdio.h>

#include<string.h>

int main()

{

 char *s[ ] = {"we will teach you how to" ,"Move a mountain " ,"Level a building " ,"Erase the past","Make a million"};

 int i,j,count=0;

 for(i=0;*(s+i)!='\0';i++)

  for(j=0;*((*(s+i))+j)!='\0';j++)

    if(s[i][j]=='e')

{

     printf("%s\n",s[i]);

      count=count+1;

}

 printf("%d",count);

 return 0;

}
