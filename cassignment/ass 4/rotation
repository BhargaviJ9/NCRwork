#include<stdio.h>

#include<string.h>

int main()

{

 char str[20],str1[20],k;

 int i,j=1,n;

 printf("enter a string");

 scanf("%s",str);

 n=strlen(str);

 while(j<n)

 {

   i=0;

   k=str[0];

   while(i<strlen(str)-1)

   {

   str[i]=str[i+1];

   i=i+1;

   }

   str[strlen(str)-1]=k;

   printf("%s\n",str);

   j++;

  }

 return 0;

}
